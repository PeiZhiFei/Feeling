<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:state_pressed="true">
        <shape>

            <corners android:radius="5dp" />
            <stroke android:width="0.5dp" android:color="@color/white" android:dashGap="3dp" android:dashWidth="6dp" />


        </shape>
    </item>
    <item>
        <shape>

            <corners android:radius="5dp" />
            <stroke android:width="0.5dp" android:color="@color/orange" android:dashGap="3dp" android:dashWidth="6dp" />


        </shape>
    </item>
</selector>